module johnscirc_vlg_vec_tst();
// constants                                           
// general purpose registers
reg Clk;
reg Clrn;
// wires                                               
wire [0:6] leds;
wire [3:0] Mag;
wire [0:2] Q;

wire sampler;                             

// assign statements (if any)                          
johnscirc i1 (
// port map - connection between master ports and signals/registers   
	.Clk(Clk),
	.Clrn(Clrn),
	.leds(leds),
	.Mag(Mag),
	.Q(Q)
);

// Clk
initial
begin
	Clk = 1'b0;
	Clk = #40000 1'b1;
	Clk = #20000 1'b0;
	Clk = #20000 1'b1;
	Clk = #20000 1'b0;
	Clk = #20000 1'b1;
	Clk = #20000 1'b0;
	Clk = #20000 1'b1;
	Clk = #20000 1'b0;
	Clk = #20000 1'b1;
	Clk = #20000 1'b0;
	Clk = #40000 1'b1;
end 

// Clrn
initial
begin
	Clrn = 1'b1;
	Clrn = #240000 1'b0;
	Clrn = #20000 1'b1;
end 

johnscirc_vlg_sample_tst tb_sample (
	.Clk(Clk),
	.Clrn(Clrn),
	.sampler_tx(sampler)
);

johnscirc_vlg_check_tst tb_out(
	.leds(leds),
	.Mag(Mag),
	.Q(Q),
	.sampler_rx(sampler)
);
endmodule

