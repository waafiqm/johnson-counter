`timescale 1 ps/ 1 ps
module johnscirc_vlg_sample_tst(
	Clk,
	Clrn,
	sampler_tx
);
input  Clk;
input  Clrn;
output sampler_tx;

reg sample;
time current_time;
always @(Clk or Clrn)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

